{
  "cron": [
    {
      "command": "python cli.py load --env demo --universe-name ecospheres",
      "schedule": "42 4 * * *"
    },
    {
      "command": "python cli.py load --env prod --universe-name univers-ecospheres",
      "schedule": "42 3 * * *"
    }
  ],
  "healthchecks": {
    "web": [
      {
        "type": "startup",
        "name": "web check",
        "description": "Checking if the app responds to the /",
        "path": "/",
        "attempts": 3,
        "content": "ok"
      }
    ]
  }
}
